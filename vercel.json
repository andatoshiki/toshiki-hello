{
  "headers": [
    {
      "source": "/:all*(ttf|otf|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/github",
      "destination": "https://github.com/toshiki"
    },
    {
      "source": "/gh",
      "destination": "https://github.com/toshiki"
    },
    {
      "source": "/github/:slug*",
      "destination": "https://github.com/toshiki/:slug*"
    },
    {
      "source": "/gh/:slug*",
      "destination": "https://github.com/toshiki/:slug*"
    }
  ]
}
